    "    [0,0,0,1,2],\n",
    "]\n",
    "\n",
    "df = pd.DataFrame(data, index=score, columns=score)\n",
    "df.columns.name = \"X\"\n",
    "df.index.name = \"Y\"\n",
    "\n",
    "pmf = df / df.values.sum()\n",
    "\n",
    "print(pmf)"
